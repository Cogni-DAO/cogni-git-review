{
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "gitleaks",
          "semanticVersion": "v8.0.0",
          "informationUri": "https://github.com/gitleaks/gitleaks",
          "rules": [
            {
              "id": "aws-access-key",
              "shortDescription": {
                "text": "AWS Access Key"
              }
            },
            {
              "id": "generic-api-key",
              "shortDescription": {
                "text": "Generic API Key"
              }
            }
          ]
        }
      },
      "results": [
        {
          "message": {
            "text": "aws-access-key has detected secret for file src/config.js."
          },
          "ruleId": "aws-access-key",
          "level": "error",
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/config.js"
                },
                "region": {
                  "startLine": 12,
                  "startColumn": 15,
                  "endLine": 12,
                  "endColumn": 35,
                  "snippet": {
                    "text": "FAKE_AWS_KEY_DO_NOT_USE"
                  }
                }
              }
            }
          ]
        },
        {
          "message": {
            "text": "generic-api-key has detected secret for file scripts/deploy.sh."
          },
          "ruleId": "generic-api-key", 
          "level": "warning",
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "scripts/deploy.sh"
                },
                "region": {
                  "startLine": 44,
                  "startColumn": 10,
                  "endLine": 44,
                  "endColumn": 40,
                  "snippet": {
                    "text": "SK_TEST_FAKE_key_DO_NOT_USE"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  ]
}