{
  "version": "2.1.0",
  "$schema": "https://raw.githubusercontent.com/oasis-tcs/sarif-spec/master/Schemata/sarif-schema-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeQL",
          "version": "2.15.3",
          "rules": [
            {
              "id": "js/unused-local-variable",
              "name": "Unused local variable",
              "shortDescription": {
                "text": "Unused local variable"
              }
            },
            {
              "id": "js/sql-injection",
              "name": "Database query built from user-controlled sources",
              "shortDescription": {
                "text": "Building a database query from user-controlled sources is vulnerable to insertion of malicious code by the user."
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "js/unused-local-variable",
          "level": "warning",
          "message": {
            "text": "Unused local variable 'temp'."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/main.js"
                },
                "region": {
                  "startLine": 15,
                  "startColumn": 9,
                  "endLine": 15,
                  "endColumn": 13
                }
              }
            }
          ]
        },
        {
          "ruleId": "js/sql-injection",
          "level": "error",
          "message": {
            "text": "Query built from user-controlled source."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "/builds/myorg/myrepo/src/db.js"
                },
                "region": {
                  "startLine": 28,
                  "startColumn": 5,
                  "endLine": 28,
                  "endColumn": 32
                }
              }
            }
          ],
          "correlationGuid": "12345678-1234-5678-9012-123456789012"
        },
        {
          "ruleId": "js/missing-error-handling",
          "level": "note",
          "message": {
            "text": "Consider adding error handling for this operation."
          },
          "locations": []
        }
      ]
    }
  ]
}